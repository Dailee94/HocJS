function soHoanHao(n) {
    if (n <= 1) return false;
    let tong = 0;
    for (let i = 1; i < n; i++){
        if (n % i === 0) {
            tong += i;
        }
    }
    if (tong === n) {
        console.log(n);
    } else {
        console.log(`${n} khong phai so hoan hao`)
    }
}
let n = Number(prompt('nhap n:'))
soHoanHao(n);
